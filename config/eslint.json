{
  "env": {
    "node": true
  },

  "rules": {
    "no-console": 2,
    "no-comma-dangle": 0,
    "no-debugger": 1,
    "no-empty": 1,
    "no-obj-calls": 2,
    "no-unreachable": 2,
    "use-isnan": 2,
    "no-dupe-keys": 2,
    "no-empty-class": 2,
    "no-func-assign": 2,
    "no-control-regex": 2,
    "no-ex-assign": 2,


    "no-alert": 0,
    "no-caller": 2,
    "curly": 2,
    "eqeqeq": 2,
    "dot-notation": 1,
    "no-native-reassign": 2,
    "no-catch-shadow": 2,
    "no-eval": 2,
    "no-with": 2,
    "no-undef": 2,
    "no-undef-init": 2,
    "no-delete-var": 2,
    "no-return-assign": 2,
    "no-label-var": 2,
    "wrap-iife": 2,
    "no-self-compare": 1,
    "no-eq-null": 2,
    "no-multi-str": 2,
    "no-loop-func": 2,
    "no-empty-label": 2,
    "unnecessary-strict": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": [1, "local"],
    "no-proto": 1,
    "no-iterator": 2,
    "no-else-return": 1,
    "no-shadow": 2,
    "no-use-before-define": 2,
    "no-redeclare": 2,
    "no-invalid-regexp": 2,

    "no-floating-decimal": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-div-regex": 1,
    "strict": 0,
    "no-global-strict": 0,


    "camelcase": 1,
    "new-cap": 2,
    "quote-props": 0,
    "semi": 2,
    "no-ternary": 0,
    "no-nested-ternary": 0,
    "consistent-this": [1, "self"],
    "one-var": 0,
    "no-mixed-requires": [0, false],
    "no-wrap-func": 2,
    "complexity": [0, 11],
    "new-parens": 2,
    "no-spaced-func": 1,
    "quotes": [1, "single"],
    "wrap-regex": 0,
    "sort-vars": 0,
    "no-new-array": 2,
    "no-new-object": 2,
    "no-shadow-restricted-names": 2,
    "no-negated-in-lhs": 2,
    "no-extend-native": 1,
    "brace-style": 2,
    "block-scoped-var": 0,
    "space-return-throw-case": 2,
    "space-infix-ops": 0,
    "space-unary-word-ops": 2,


    "no-bitwise": 0,
    "no-plusplus": 1,
    "guard-for-in": 0,
    "max-params": [1, 4],
    "max-depth": [1, 4],
    "max-len": [1, 80, 4],
    "no-extra-parens": 1,
    "no-extra-semi": 2,


    "no-implied-eval": 2,
    "no-fallthrough": 2,
    "no-new-wrappers": 2,
    "no-sync": 0,
    "no-underscore-dangle": 2,
    "no-script-url": 0,
    "radix": 0,
    "regex-spaces": 2
  }
}